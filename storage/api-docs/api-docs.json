                },
                "responses": {
                    "201": {
                        "description": "Successful registration",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "description": "User's JWT token",
                                            "type": "string",
                                            "example": "eyJpc3MiOiJodHRwOi8vbG9jYWxob3N0L2FwaS91c2V"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
